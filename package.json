    "react-icons": "^4.1.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "slugify": "^1.4.6",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "husky": ">=4.3.8",
